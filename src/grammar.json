{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "molviewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "component"
      }
    },
    "component": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "node"
          }
        }
      ]
    },
    "_indent": {
      "type": "PATTERN",
      "value": "\\t+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_$][a-zA-Z0-9_$]*"
    },
    "component_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "property_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "parameter"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "parameter": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "ALIAS",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "!"
                },
                {
                  "type": "STRING",
                  "value": "?"
                }
              ]
            },
            "named": true,
            "value": "parameter_marker"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          }
        ]
      }
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "primitive_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        },
        {
          "type": "STRING",
          "value": "null"
        },
        {
          "type": "PATTERN",
          "value": "-?\\d+(\\.\\d+)?"
        }
      ]
    },
    "list_marker": {
      "type": "STRING",
      "value": "/"
    },
    "dict_marker": {
      "type": "STRING",
      "value": "*"
    },
    "null_value": {
      "type": "STRING",
      "value": "-"
    },
    "binding": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "STRING",
          "value": "<=>"
        }
      ]
    },
    "localization_marker": {
      "type": "STRING",
      "value": "@"
    },
    "component_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "component_name"
        }
      ]
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "property_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "binding"
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "primitive_literal"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "string_literal"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "component_name"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "localization_marker"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "list_marker"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "dict_marker"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "null_value"
                },
                {
                  "type": "SYMBOL",
                  "name": "string_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "primitive_literal"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "node": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_indent"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "component_declaration"
              },
              {
                "type": "SYMBOL",
                "name": "property"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "node"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
